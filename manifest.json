{
  "manifest_version": 3,
  "name": "boost-gigazine",
  "version": "0",
  "description": "Boost gigazine.net",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "contentSettings",
    "declarativeNetRequest",
    "userScripts"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "0",
      "path": "0.json",
      "enabled": true
    }]
  },
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [{
    "css": ["main.css"],
    "matches": ["https://gigazine.net/"],
    "run_at": "document_start"
  }]
}